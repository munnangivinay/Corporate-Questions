def is_prime(num):
    if num <= 1:
        return False
    if num == 2:
        return True
    if num % 2 == 0:
        return False
    for i in range(3, int(num**0.5) + 1, 2):
        if num % i == 0:
            return False
    return True

m = int(input("enter number m : "))
n = int(input("enter number n : "))

if m > n:
    print("Invalid range: m should be less than or equal to n")
else:
    total = 0
    for num in range(m, n + 1):
        if is_prime(num):
            total += num
    print(f"Sum of prime numbers between {m} and {n} is {total}")
